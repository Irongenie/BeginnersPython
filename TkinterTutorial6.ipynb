   "source": [
    "from tkinter import *\n",
    "\n",
    "root = Tk()\n",
    "\n",
    "def click1():\n",
    "    print(\"Nice click you have there\")\n",
    "\n",
    "\n",
    "\n",
    "fun_button = Button(root, text=\"Click me, now!!!\", command=click1) #Command = can call function\n",
    "fun_button.pack()\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "root.mainloop()"
   ]
